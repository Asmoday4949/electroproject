{
  "name": "electron-gantt",
  "version": "1.0.0",
  "description": "simple Gantt diagram editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-win": "electron-packager . electron-gantt --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=\"HE-ARC\" --version-string.FileDescription=\"HE-ARC\" --version-string.ProductName=\"ElectroGantt\"",
    "package-linux": "electron-packager . electron-gantt --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "Bulloni Lucas, Fleury Malik",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bull0n/electroproject.git"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "electron-packager": "^12.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4"
  }
}
